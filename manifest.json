{
  "manifest_version": 2,
  "name": "FireX Proxy",
  "version": "5.1.0",
  "description": "FireX Proxy",
  "icons": {
    "16": "data/icons/icon-16.png",
    "24": "data/icons/icon-24.png",
    "32": "data/icons/icon-32.png"
  },
  "applications": {
    "gecko": {
      "id": "divanproger@gmail.com",
      "strict_min_version": "45.0"
    }
  },
  "permissions": [
    "proxy",
    "storage",
    "*://free-proxy-list.net/*"
  ],
  "browser_action": {
    "browser_style": false,
    "default_icon": "data/icons/icon-32.png",
    "default_title": "FireX Proxy",
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": [
      "addon/address.js",
      "addon/addresses.js",
      "addon/connector.js",
      "addon/proxyAdapter.js",
      "addon/freeProxyList.js",
      "addon/helpers.js",
      "addon/background.js"
    ]
  }
}